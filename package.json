{
  "name": "seasmoke-plugin",
  "version": "1.0.40",
  "description": "",
  "private": true,
  "scripts": {
    "build:plugin": "webpack --env entry=main dir=dist && rm dist/main.js.LICENSE.txt",
    "build:extension": "webpack --env entry=extension dir=extension/scripts && rm extension/scripts/inject.js.LICENSE.txt",
    "pretty-quick": "pretty-quick --staged",
    "prepare": "husky install",
    "deploy:prod": "export AWS_PROFILE=default && aws s3 sync ./dist s3://seasmoke --acl public-read --cache-control max-age=120",
    "deploy:dev": "export AWS_PROFILE=default && aws s3 sync ./dist s3://seasmoke-dev --acl public-read --cache-control max-age=120"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/history": "^5.0.0",
    "@types/papaparse": "^5.3.5",
    "@types/uuid": "^8.3.4",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@medv/finder": "^2.1.0",
    "@types/chrome": "^0.0.200",
    "@types/xlsx": "^0.0.36",
    "dotenv": "^16.0.3",
    "uuid": "^8.3.2",
    "whatwg-fetch": "^3.6.2",
    "xlsx": "^0.18.5"
  }
}
